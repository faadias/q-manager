{
  "middlewares": {
    "cors": {
      "origin": "*",
      "methods": "GET,HEAD,PUT,PATCH,POST,DELETE,OPTIONS",
      "preflightContinue": false,
      "optionsSuccessStatus": 204,
      "exposedHeaders": ["Content-Disposition"]
    },
    "json": {
      "limit": "20mb"
    },
    "urlencoded": {
      "extended": false,
      "limit": "20mb"
    }
  },
  "logger": {
    "enabled": true,
    "level": "debug"
  }
}
