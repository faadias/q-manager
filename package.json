{
  "name": "q-manager",
  "version": "1.0.0",
  "description": "",
  "main": "dist/src/main.js",
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "start:dev": "cross-env NODE_ENV=development CLIMEM=8777 ts-node-dev -r climem src/main.ts",
    "start:test": "cross-env NODE_ENV=test ts-node src/main.ts",
    "start": "npm run build && node .",
    "climem": "npx climem 8777",
    "lint:check": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "test:unit": "cross-env NODE_ENV=test jest",
    "test:stress": "cross-env NODE_ENV=test npx autocannon -c 100 -d 20 -w 4 --renderStatusCodes -W [ -c 1 -d 4 ] -m 'POST' -i './test/fixture/large.json' -H 'Content-Type: application/json' http://localhost:3000/test"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "@types/amqplib": "^0.8.2",
    "@types/config": "^3.3.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.4",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "autocannon": "^7.9.0",
    "climem": "^1.0.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.7",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.6",
    "amqplib": "^0.10.2",
    "body-parser": "^1.20.0",
    "config": "^3.3.7",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "http-status-codes": "^2.2.0",
    "uuid": "^8.3.2"
  }
}
